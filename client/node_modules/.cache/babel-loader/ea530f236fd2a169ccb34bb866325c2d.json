\u0437\\u0438\\u043D\\u0435, \\u0432\\u044B \\u0441\\u043C\\u043E\\u0436\\u0435\\u0442\\u0435 \\u0431\\u044B\\u0442\\u044C \\u043C\\u043E\\u0434\\u043D\\u044B\\u043C \\u0438 \\u0441\\u0442\\u0438\\u043B\\u044C\\u043D\\u044B\\u043C \\u043A\\u0430\\u043A \\u043E\\u0441\\u0435\\u043D\\u044C\\u044E-\\u0437\\u0438\\u043C\\u043E\\u0439, \\u0442\\u0430\\u043A \\u0438 \\u0432\\u0435\\u0441\\u043D\\u043E\\u0439-\\u043B\\u0435\\u0442\\u043E\\u043C. \\u041F\\u0440\\u043E\\u0441\\u0442\\u043E \\u043D\\u0430\\u0431\\u0435\\u0440\\u0438\\u0442\\u0435 \\u043D\\u043E\\u043C\\u0435\\u0440 \\u043D\\u0430\\u0448\\u0435\\u0433\\u043E \\u0442\\u0435\\u043B\\u0435\\u0444\\u043E\\u043D\\u0430, \\u0438 \\u043C\\u044B \\u043F\\u043E\\u043C\\u043E\\u0436\\u0435\\u043C \\u0432\\u0430\\u043C \\u043E\\u043F\\u0440\\u0435\\u0434\\u0435\\u043B\\u0438\\u0442\\u044C\\u0441\\u044F \\u0441 \\u043F\\u043E\\u043A\\u0443\\u043F\\u043A\\u043E\\u0439.\", \" \")))));\n};\n\nexport default About;","map":{"version":3,"sources":["/Users/blokhinsergey/Desktop/StudyIt/React/netology/e-store/client/src/pages/about.js"],"names":["React","Navbar","Banner","About"],"mappings":";AAAA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,sBAAnB;AACA,OAAOC,MAAP,MAAmB,UAAnB;AAEA,OAAO,cAAP;;AAEA,MAAMC,KAAK,GAAG,MAAM;AAClB,sBACE;AAAM,IAAA,SAAS,EAAC,WAAhB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kBACE;AAAK,IAAA,SAAS,EAAC,KAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kBACE;AAAK,IAAA,SAAS,EAAC,KAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kBACE,oBAAC,MAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IADF,eAEE;AAAS,IAAA,SAAS,EAAC,WAAnB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kBACE;AAAI,IAAA,SAAS,EAAC,aAAd;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,+DADF,eAEE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qoBAFF,eAME;AAAG,IAAA,SAAS,EAAC,gBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qLANF,eAOE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kBACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,o8CADF,eAQE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,8uBARF,eAaE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,w6BAbF,eAmBE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,oNAnBF,eAoBE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,0VApBF,CAPF,eA+BE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACG,GADH,kkBAGoD,GAHpD,CA/BF,eAoCE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,k8CAKsD,GALtD,CApCF,CAFF,CADF,CADF,CADF;AAqDD,CAtDD;;AAwDA,eAAeA,KAAf","sourcesContent":["import React from \"react\";\nimport Navbar from \"../components/navbar\";\nimport Banner from \"./banner\";\n\nimport \"../index.css\";\n\nconst About = () => {\n  return (\n    <main className=\"container\">\n      <div className=\"row\">\n        <div className=\"col\">\n          <Banner />\n          <section className=\"top-sales\">\n            <h2 className=\"text-center\">О магазине</h2>\n            <p>\n              В Интернете можно встретить немало магазинов, предлагающих\n              аксессуары. Но именно к нам хочется возвращаться снова и снова.\n            </p>\n            <p className=\"h4 text-center\">Мы предлагаем вам особые условия:</p>\n            <ol>\n              <li>\n                Индивидуальный подход специалиста. Когда поступает новая\n                коллекция обуви весна-лето или же коллекция обуви осень-зима –\n                покупателям бывает трудно сориентироваться во всем многообразии\n                новинок. Наш менеджер по телефону поможет вам определиться с\n                товарами, подходящими именно вам.\n              </li>\n              <li>\n                Мы периодически проводим распродажи как женских и мужских, так и\n                детских моделей. Вы будете приятно удивлены ценами на аксессуары\n                в мага- зине BosaNoga.\n              </li>\n              <li>\n                У нас всегда есть из чего выбрать. Неважно, какую категорию вы\n                прос- матриваете: осень-зима, или же весна-лето – вы всегда\n                сможете найти ва- рианты, подходящие вам по внешнему виду и\n                цене.\n              </li>\n              <li>Мы несем ответственность за все товары.</li>\n              <li>\n                Молодые мамы будут рады обширному ассортименту детских моделей.\n              </li>\n            </ol>\n            <p>\n              {\" \"}\n              Если вы ищете место, где представлены обувные новинки от самых\n              известных брендов, то вы зашли по верному адресу.{\" \"}\n            </p>\n            <p>\n              У нас представлены модели для мужчин, женщин, а также детские\n              сапоги, босоножки, ботинки и туфли. Сделав заказ в нашем\n              интернет-магазине, вы сможете быть модным и стильным как\n              осенью-зимой, так и весной-летом. Просто наберите номер нашего\n              телефона, и мы поможем вам определиться с покупкой.{\" \"}\n            </p>\n          </section>\n        </div>\n      </div>\n    </main>\n  );\n};\n\nexport default About;\n"]},"metadata":{},"sourceType":"module"}